#include <stdio.h>

struct Car {
    char model[30];
    char company[30];
    float mileage;
    float price;
};

int main() {
    struct Car cars[3];
    int maxIndex = 0;

    for (int i = 0; i < 3; i++) {
        printf("\nEnter details for car %d\n", i + 1);
        printf("Model: ");
        scanf("%s", cars[i].model);
        printf("Company: ");
        scanf("%s", cars[i].company);
        printf("Mileage: ");
        scanf("%f", &cars[i].mileage);
        printf("Price: ");
        scanf("%f", &cars[i].price);

        if (cars[i].mileage > cars[maxIndex].mileage)
            maxIndex = i;
    }

    printf("\nCar with highest mileage:\n");
    printf("Model: %s\n", cars[maxIndex].model);
    printf("Company: %s\n", cars[maxIndex].company);
    printf("Mileage: %.2f\n", cars[maxIndex].mileage);
    printf("Price: %.2f\n", cars[maxIndex].price);

    return 0;
}
